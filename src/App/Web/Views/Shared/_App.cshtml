@using Microsoft.AspNetCore.Components.Web
@using Microsoft.Extensions.Hosting

<!DOCTYPE html>

<html lang="en">

<head>
    <base href="/" />
    <meta charset="utf-8" />
    <meta name="viewport" content="height=device-height,width=device-width,initial-scale=1" />
    <meta name="application-name" content="Wadio" />
    <meta name="color-scheme" content="dark" />
    <meta name="description" content="A music app, powered by radio-browser" />
    <meta name="keywords" content="music,radio-browser,radiobrowser-api" />
    <meta name="theme_color" content="#4b004f" />

    <link rel="preconnect" href="//fonts.googleapis.com" crossorigin />
    <link rel="preconnect" href="//fonts.gstatic.com" crossorigin />

    <link href="/_framework/blazor.boot.json" rel="prefetch" as="fetch" asp-append-version="true" />
    <link href="/_framework/dotnet.runtime.js" rel="prefetch" as="script" asp-append-version="true" />
    <link href="/_framework/dotnet.js" rel="prefetch" as="script" asp-append-version="true" />
    <link href="/_framework/blazor.webassembly.js" rel="preload" as="script" asp-append-version="true" />

    <link href="https://fonts.googleapis.com/css2?family=Material+Symbols+Rounded:opsz,wght,FILL,GRAD@20..48,100..700,0..1,-50..200&family=Rubik+Glitch+Pop&family=Nunito:ital,wght@0,200..1000;1,200..1000&family=IBM+Plex+Mono:wght@100;200;300;400;500;600;700" rel="preload" as="style" crossorigin />
    <link href="/Wadio.App.Web.styles.css" rel="preload" as="style" asp-append-version="true" />

    <environment names="@Environments.Development">
        <link href="https://cdn.jsdelivr.net/npm/@@tailwindcss/browser@4" rel="preload" as="script" crossorigin />
    </environment>

    <link href="/manifest.json" rel="manifest" type="application/json" asp-append-version="true" />
    <link href="/osd/spec.xml" rel="search" type="application/opensearchdescription+xml" title="Wadio Search" />

    <link href="/icon.svg" rel="icon" sizes="any" type="image/svg+xml" asp-append-version="true" />
    <link href="/icon-16.png" rel="icon" sizes="16x16" type="image/png" asp-append-version="true" />
    <link href="/icon-24.png" rel="icon" sizes="24x24" type="image/png" asp-append-version="true" />
    <link href="/icon-48.png" rel="icon" sizes="48x48" type="image/png" asp-append-version="true" />
    <link href="/icon-64.png" rel="icon" sizes="64x64" type="image/png" asp-append-version="true" />
    <link href="/icon-96.png" rel="icon" sizes="96x96" type="image/png" asp-append-version="true" />
    <link href="/icon-128.png" rel="icon" sizes="128x128" type="image/png" asp-append-version="true" />
    <link href="/icon-154.png" rel="icon" sizes="154x154" type="image/png" asp-append-version="true" />
    <link href="/icon-256.png" rel="icon" sizes="256x256" type="image/png" asp-append-version="true" />
    <link href="/icon-512.png" rel="icon" sizes="512x512" type="image/png" asp-append-version="true" />
    <link href="/icon-16.png" rel="apple-touch-icon" sizes="16x16" type="image/png" asp-append-version="true" />
    <link href="/icon-24.png" rel="apple-touch-icon" sizes="24x24" type="image/png" asp-append-version="true" />
    <link href="/icon-48.png" rel="apple-touch-icon" sizes="48x48" type="image/png" asp-append-version="true" />
    <link href="/icon-64.png" rel="apple-touch-icon" sizes="64x64" type="image/png" asp-append-version="true" />
    <link href="/icon-96.png" rel="apple-touch-icon" sizes="96x96" type="image/png" asp-append-version="true" />
    <link href="/icon-128.png" rel="apple-touch-icon" sizes="128x128" type="image/png" asp-append-version="true" />
    <link href="/icon-154.png" rel="apple-touch-icon" sizes="154x154" type="image/png" asp-append-version="true" />
    <link href="/icon-256.png" rel="apple-touch-icon" sizes="256x256" type="image/png" asp-append-version="true" />
    <link href="/icon-512.png" rel="apple-touch-icon" sizes="512x512" type="image/png" asp-append-version="true" />

    <component type="@typeof(HeadOutlet)" render-mode="WebAssemblyPrerendered" />
    <link href="/Wadio.App.Web.styles.css" rel="stylesheet" type="text/css" asp-append-version="true" />
    <link href="https://fonts.googleapis.com/css2?family=Material+Symbols+Rounded:opsz,wght,FILL,GRAD@20..48,100..700,0..1,-50..200&family=Rubik+Glitch+Pop&family=Nunito:ital,wght@0,200..1000;1,200..1000&family=IBM+Plex+Mono:wght@100;200;300;400;500;600;700" rel="stylesheet" type="text/css" crossorigin />

    <environment names="@Environments.Development">
        <script src="https://cdn.jsdelivr.net/npm/@@tailwindcss/browser@4" crossorigin></script>
    </environment>

    <link href="/Interop/LocalStorage.module.js" rel="prefetch" as="script" />
    <link href="/Interop/Player.module.js" rel="prefetch" as="script" />
</head>

<body>
    <component type="@typeof(AppRoot)" render-mode="WebAssemblyPrerendered" />

    <persist-component-state />
    <script src="/_framework/blazor.webassembly.js" type="text/javascript" asp-append-version="true"></script>
</body>

</html>
